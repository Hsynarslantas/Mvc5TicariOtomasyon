@model CommercialAutomation.Models.Classes.Department
@{
    ViewBag.Title = "CreateDepartmant";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<h2>Yeni Departman Ekleme Sayfası</h2>
<br /><br />
<form method="post" class="form-group">
    @Html.Label("DepartmentName", "Departman Adı")
    @Html.TextBoxFor(x => x.DepartmentName, new { @class = "form-control" })
    <br /><br />
    <button class="btn btn-info"style="margin-left:20px;">Ekle</button>
</form>

